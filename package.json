{
  "name": "react-test-buffet",
  "version": "3.0.0",
  "description": "Complete example of building UI with TDD and React",
  "keywords": [
    "react",
    "tdd",
    "testing",
    "unit",
    "acceptance",
    "mocha",
    "chai",
    "sinon",
    "selenium",
    "webdriverio",
    "coverage",
    "istanbul",
    "nyc",
    "docker"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:NiGhTTraX/react-test-buffet.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "scripts": {
    "test": "npm run test:unit --silent && npm run test:acceptance --silent",
    "test:unit": "NODE_ENV=tests nyc mocha --compilers js:babel-register --reporter min tests/unit/helpers/index.node.js \"tests/unit/**/*.spec.js\"",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:debug": "npm run build -- ./tests/unit/webpack.config.unit.js -w",
    "test:acceptance": "./tests/acceptance/run.sh",
    "test:acceptance:debug": "./tests/acceptance/debug.sh",
    "_test:acceptance": "wdio tests/acceptance/wdio.conf.js",
    "_test:acceptance:debug": "wdio tests/acceptance/wdio.conf.debug.js",
    "build:bail": "webpack --progress --bail --config",
    "build": "webpack --progress --config",
    "lint": "npm run lint:files -- .",
    "lint:files": "eslint --ext js --ext jsx",
    "postinstall": "./tools/install-git-hooks.sh",
    "start": "node tests/dev-server.js"
  },
  "dependencies": {
    "classnames": "~2.2.5",
    "react": "~15.3.2",
    "react-dom": "~15.3.2",
    "todomvc-app-css": "~2.0.6"
  },
  "devDependencies": {
    "babel-core": "~6.22.1",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.4",
    "babel-plugin-istanbul": "~3.1.2",
    "babel-plugin-transform-class-properties": "~6.9.0",
    "babel-plugin-transform-object-rest-spread": "~6.20.2",
    "babel-preset-env": "~1.1.8",
    "babel-preset-react": "~6.5.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-register": "~6.9.0",
    "chai": "~3.5.0",
    "css-loader": "~0.23.1",
    "eslint": "~3.13.0",
    "eslint-config-airbnb": "~14.0.0",
    "eslint-import-resolver-webpack": "~0.8.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~3.0.2",
    "eslint-plugin-react": "~6.9.0",
    "express": "~4.14.0",
    "jquery": "~2.2.4",
    "jsdom": "~9.2.1",
    "lodash.isequal": "~4.5.0",
    "lodash.merge": "~4.4.0",
    "mocha": "~3.2.0",
    "nyc": "~10.0.0",
    "react-addons-test-utils": "~15.3.2",
    "sinon": "https://github.com/uberVU/Sinon.JS.git",
    "sinon-chai": "~2.8.0",
    "style-loader": "~0.13.1",
    "wdio": "~0.3.3",
    "wdio-mocha-framework": "~0.5.8",
    "webdriverio": "~4.6.1",
    "webpack": "~1.13.1",
    "webpack-dev-middleware": "~1.9.0",
    "webpack-hot-middleware": "~2.15.0"
  }
}
